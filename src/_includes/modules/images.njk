  <!-- ##### Testimonial Area Start ##### -->
  <section class="gallery-area bg-img  section-padding-100 jarallax" style="background-color: #f2f4f5;">
    <div class="container">
    <div class="section-heading text-center">
            <p>Recent Work</p>
            <h2><span>Take a moment to view</span> our latest work</h2>
            <img src="/assets/img/core-img/decor2.png" alt="">
          </div>
      <div class="row">
        <div class="col-12">
          <!-- Section Heading -->
          <div class="section-heading text-center">
            <p>Featured Products</p>
            <h2><span>Our Product</span> Are Highest Quality</h2>
            <img src="/assets/img/core-img/decor2.png" alt="">
          </div>
        </div>
      </div>

      <div class="row">
          <!-- Testimonial Slides -->
          <div class="gallery-slides owl-carousel">

              {% for item in gallery.image %}
            <!-- Single Slide -->
            <div class="single-slide">
                <div class="gallery-thumbnail">
                    <a data-fancybox="gallery" href="{{item}}"><img src="{{item}}" alt=""></a>
                </div>
            </div>
              {% endfor %}


          </div>

      </div>

      <div class="row">
        <div class="col-12">
          <div class="gotoshop-btn text-center">
            <a href="shop.html" class="btn famie-btn">Go to Store</a>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- ##### Our Products Area End ##### -->